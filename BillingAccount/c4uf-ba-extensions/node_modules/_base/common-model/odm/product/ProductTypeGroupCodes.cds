namespace sap.odm.product;

using { sap.odm.common.codeList } from '../common';

/**
 * A specific group of product types.
 */
type ProductTypeGroupCode: association to one ProductTypeGroupCodes;

/**
 * Groups product types according to their business meaning,
 * e.g. as a tangible, service or subscription product.
 */
@odm.doc.SAPDelivered: true
@odm.doc.extensible: false
entity ProductTypeGroupCodes: codeList {
  /**
   * Product type group code.
   */
  key code: String(1);
};
