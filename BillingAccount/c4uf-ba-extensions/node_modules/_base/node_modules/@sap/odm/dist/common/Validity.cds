namespace sap.odm.common;

/**
 * Aspect for entities with validFrom and validTo date, but w/o
 * making validFrom to a key (in comparison to temporal aspect).
 */
aspect Validity {
  /**
   * Begin date of validity period.
   */
  validFrom: Date;
  /**
   * End date of validity period.
   */
  validTo: Date;
};
