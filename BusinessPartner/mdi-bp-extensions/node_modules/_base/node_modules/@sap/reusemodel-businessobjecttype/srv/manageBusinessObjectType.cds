namespace cds.foundation.sap.businessobjecttype;

using from '../db/BusinessObjectTypeModel';

service businessObjectType{
	
	@Capabilities: { Insertable:true, Updatable:true, Deletable:true }
	view ObjectTypes as select from businessobjecttype.ObjectType{
		key objectTypeCode,		
		businessObject,
		isProjection,
		mainObject,
		BusConfignVersion,
		'                 ' as BusConfignVersionOut:String(17),
		description	: redirected to ObjectTypeTexts,
		localizedDescription.description as localizedDescription
	} excluding {
		description,
		localizedDescription		
	};
	
	@Capabilities: { Insertable:true, Updatable:true, Deletable:true }
	entity ObjectTypeTexts as projection on  businessobjecttype.ObjectTypeText{*};
		
}