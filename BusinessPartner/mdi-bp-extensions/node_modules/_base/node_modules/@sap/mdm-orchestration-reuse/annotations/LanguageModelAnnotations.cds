using mdo.manageDistributions from '../srv/manageDistributions';

annotate manageDistributions.Languages with @(

	UI.HeaderInfo: {
		TypeName: '{i18n>Language}',
		TypeNamePlural: '{i18n>Languages}',
		Title: {
			Value: LanguageTwoLetterISOCode
		},
	},

	Capabilities.FilterRestrictions.NonFilterableProperties: [
		BusConfignVersion
	],

	UI.FieldGroup#ConfigurationData: {
		Data: [
			{$Type: 'UI.DataField', Label: '{i18n>LanguageTwoLetterISOCode}', Value: LanguageTwoLetterISOCode}
		]
	},

	UI.Facets: [
		{$Type: 'UI.CollectionFacet', Label: '{i18n>Language}',
			Facets: [
				{$Type: 'UI.ReferenceFacet', Target: '@UI.FieldGroup#ConfigurationData'},
			],
		},
		{$Type: 'UI.ReferenceFacet', ID: 'LanguageText', Label: '{i18n>LanguagesTexts}', Target: 'text/@UI.LineItem' }
	],

	UI.SelectionFields: [ 
		LanguageTwoLetterISOCode,
		text.LanguageName
	],

	UI.LineItem: [
		{$Type: 'UI.DataField', Label: '{i18n>LanguageTwoLetterISOCode}', Value: 'LanguageTwoLetterISOCode'},
		{$Type: 'UI.DataField', Label: '{i18n>LanguageName}', Value: 'text/LanguageName'}
	]
){
	LanguageTwoLetterISOCode @(
		Common.Text: {$value: description, ![@UI.TextArrangement]: #TextFirst},
		Common.ValueList: {
			CollectionPath: 'LanguagesWithTexts',
			Label: 'Test',
			Parameters: [
				{$Type: 'Common.ValueListParameterOut', LocalDataProperty: 'LanguageTwoLetterISOCode', ValueListProperty: 'LanguageTwoLetterISOCode'},
				{$Type: 'Common.ValueListParameterInOut', LocalDataProperty: 'text_LanguageName', ValueListProperty: 'LanguageName'},
			]
		},
		Common.FieldControl: #ReadOnly
	);

	BusConfignVersion @(
		Common.FieldControl: #Hidden
	);
};
