using mdo.manageDistributions from '../srv/manageDistributions';

annotate manageDistributions.DependentObjectAPIs with @(
		Common.SemanticKey: [BusinessObjectType],
		UI: { 
			LineItem: [ 
				{$Type: 'UI.DataField', Value: BusinessObjectType, ![@UI.Importance]: #High},
				{$Type: 'UI.DataField', Value: API, ![@UI.Importance]: #High},
				{$Type: 'UI.DataField', Value: APIVersion, ![@UI.Importance]: #Medium}
			],

			HeaderInfo: {
				TypeName: '{i18n>dependentObjectAPI}',
				TypeNamePlural: '{i18n>dependentObjectAPIPlural}'
			}
		}
){
	API @Common.Label: '{i18n>api}';
	APIVersion @Common.Label: '{i18n>apiVersion}';
	BusinessObjectType @Common.Label: '{i18n>objectTypeCode}';
	CreatedByUser @UI.Hidden;
	CreationDate @UI.Hidden;
	LastChangedByUser @UI.Hidden;
	LastChangeDate @UI.Hidden;
	Id @UI.Hidden;
	To_Parent @UI.Hidden;
};