using mdo.manageDistributions from '../srv/manageDistributions';

annotate manageDistributions.ObjectTypes with @(
	UI: {
		LineItem: [ 
			{$Type: 'UI.DataField', Value: objectTypeCode, ![@UI.Importance]: #Medium},
			{$Type: 'UI.DataField', Value: localizedDescription, ![@UI.Importance]: #High}
		]
	}
){
	BusConfignVersion @(
		UI.HiddenFilter: true
	);
	
	objectTypeCode @(
		UI.HiddenFilter: true,
		Common: {
			Label: '{i18n>objectTypeCode}',
			Text: localizedDescription,
		}
	);

	businessObject @(
		UI.HiddenFilter: true
	);

	isProjection @(
		UI.HiddenFilter: true
	);

	mainObject @(
		UI.HiddenFilter: true
	);
};

annotate manageDistributions.ObjectTypeDescriptions with @(
	UI: {
		LineItem: [ 
			{$Type: 'UI.DataField', Value: objectTypeCode, ![@UI.Importance]: #High},
			{$Type: 'UI.DataField', Value: language, ![@UI.Importance]: #High},
			{$Type: 'UI.DataField', Value: description, ![@UI.Importance]: #High}
		]
	}
);