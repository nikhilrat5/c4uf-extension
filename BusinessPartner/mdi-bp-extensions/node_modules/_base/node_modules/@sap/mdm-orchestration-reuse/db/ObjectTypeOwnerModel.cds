namespace mdo;

using cds.foundation.sap.businessobjecttype.ObjectType from '@sap/reusemodel-businessobjecttype/db/BusinessObjectTypeModel';

entity ObjectTypeOwner {
		key ObjectTypeCode		: String(10);
		@Search.defaultSearchElement: true
		ProviderDestination		: String(256);
		IsLocalModel			: Boolean default false;
		To_BusinessObjectType 	: Association to ObjectType on To_BusinessObjectType.objectTypeCode = ObjectTypeCode;
}

@cds.persistence.skip: true
entity BusinessObjectType {
	key objectTypeCode              : String(10);
	Description                     : String(256);
}
