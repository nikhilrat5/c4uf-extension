namespace mdo;

using mdo.SubscriptionDistribution as SubscriptionDistribution from './SubscriptionDistributionModel';

   abstract entity managedObject {        
        key Id		: UUID @Common.FieldControl: #Hidden;
        To_Parent	: Association to SubscriptionDistribution @Common.FieldControl: #Hidden;        
    }

    abstract entity tracked {
    	        
        CreatedByUser		: String(128)
        	@odata.on.insert:#user 
        	@Common.FieldControl: #ReadOnly
        	@Core.Computed: true;
        
        CreationDate		: Timestamp 
        	@odata.on.insert: #now
        	@Common.FieldControl: #ReadOnly
        	@Core.Computed: true;
                
        LastChangedByUser	: String(128)
        	@odata.on.update:#user 
        	@Common.FieldControl: #ReadOnly
        	@Core.Computed: true;
                 
        LastChangeDate		: Timestamp
        	@odata.on.update:#now 
        	@Common.FieldControl: #ReadOnly
        	@Core.Computed: true;
    }
    
    abstract entity validity {
    	@odata.on.update:#now
        ValidityStartDate	: DateTime;
        ValidityEndDate		: DateTime default timestamp'9999-12-31 23:59:59.0';
    }
    
    type Id                          : UUID;
    
    type SystemAdminData {
		    CreatedByUser            : UUID;
		    CreationDateTime         : Timestamp;
		    LastChangedByUser        : UUID;
		    LastChangeDateTime       : Timestamp;
	  };
	  		
    type ValidityStartDateTime       : Timestamp;
    
    type ValidityEndDateTime         : Timestamp;
   	
	// CodeList		
	type LanguageCode				: String(2);


	abstract entity managedUUIDObject {        
        key Id		: UUID @Common.FieldControl: #Hidden;       
    }

	type Operator : String(64) enum {
			eq='sap.ui.model.FilterOperator.EQ';
			ne='sap.ui.model.FilterOperator.NE';
			ge='sap.ui.model.FilterOperator.GE';
			gt='sap.ui.model.FilterOperator.GT';
			le='sap.ui.model.FilterOperator.LE';
			lt='sap.ui.model.FilterOperator.LT';
			cp='sap.ui.model.FilterOperator.Contains';
			np='sap.ui.model.FilterOperator.NotContains';
			bt='sap.ui.model.FilterOperator.BT';
			nb='sap.ui.model.FilterOperator.NB';
	}

	abstract entity filterable {
		LowValue		         : String(512);
		HighValue		         : String(512);
		Operator		         : Operator;
		sign			         : Boolean default false;
		AttributeDataType        : DataType;
	}
	
	type BusinessSystemName: String(32);	

	type DataType : String(32) enum {
		guid			= 'Edm.Guid';
		binary			= 'Edm.Binary';
		boolean			= 'Edm.Boolean';
		byte			= 'Edm.Byte';
		dateTime		= 'Edm.DateTime';
		decimal			= 'Edm.Decimal';
		double			= 'Edm.Double';
		single			= 'Edm.Single';
		int16			= 'Edm.Int16';
		int32			= 'Edm.Int32';
		int64			= 'Edm.Int64';
		sByte			= 'Edm.SByte';
		string			= 'Edm.String';
		time			= 'Edm.Time';
		dateTimeOffset	= 'Edm.DateTimeOffset';
	}
