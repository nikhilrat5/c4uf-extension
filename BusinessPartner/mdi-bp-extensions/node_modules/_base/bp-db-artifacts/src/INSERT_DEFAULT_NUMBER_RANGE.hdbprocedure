PROCEDURE INSERT_DEFAULT_NUMBER_RANGE
	LANGUAGE SQLSCRIPT AS
	-- SQL SECURITY INVOKER 
	-- DEFAULT SCHEMA <default_schema_name>
	-- READS SQL DATA AS
BEGIN
DECLARE isFound INTEGER = 1;
SELECT count(*) INTO isFound FROM "COM_SAP_MDM_BP_NUMBERGENERATORMODEL_NUMBERGENERATOR" 
WHERE "NUMBERRANGENAME" = 'BPRange';
 IF isFound = 0 
	THEN
       INSERT INTO "COM_SAP_MDM_BP_NUMBERGENERATORMODEL_NUMBERGENERATOR" 
		("BUSCONFIGNVERSION","BUSCONFIGNISDELETED","NUMBERRANGENAME","STARTVALUE","CURRENTVALUE","ENDVALUE")
		VALUES ('4294967296',null, 'BPRange', '1000000000', '999999999', '9999999999'); 
END IF;
END;