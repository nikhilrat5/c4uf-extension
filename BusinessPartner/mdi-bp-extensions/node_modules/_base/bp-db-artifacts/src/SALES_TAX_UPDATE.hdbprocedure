PROCEDURE SALES_TAX_UPDATE 
	LANGUAGE SQLSCRIPT AS
	-- SQL SECURITY INVOKER 
	-- DEFAULT SCHEMA <default_schema_name>
	-- READS SQL DATA AS
BEGIN
   EXEC 'UPDATE "STA" SET "CUSTOMER_ID" = "SA"."PARENT_ID" FROM "COM_SAP_MDM_BP_BUSINESSPARTNERMODEL_SALESTAX" AS "STA" INNER JOIN "COM_SAP_MDM_BP_BUSINESSPARTNERMODEL_SALESAREA" AS "SA" ON "STA"."PARENT_ID" = "SA"."ID"';
END;
