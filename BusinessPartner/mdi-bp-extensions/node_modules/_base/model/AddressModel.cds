namespace com.sap.mdm.bp;

using com.sap.mdm.bp.CommonTypes as CommonTypes from './CommonTypes';
using com.sap.mdm.bp.CountryModel as CountryModel from './CountryModel';
using com.sap.mdm.bp.RegionModel as RegionModel from './RegionModel';
using com.sap.mdm.bp.PreferredCommMediumTypeModel as PreferredCommMediumTypeModel from './PreferredCommMediumTypeModel';
using com.sap.mdm.bp.AcademicTitleModel as AcademicTitleModel from './AcademicTitleModel';
using com.sap.mdm.bp.FormOfAddressModel as FormOfAddressModel from './FormOfAddressModel';
using com.sap.mdm.bp.NameSupplementModel as NameSupplementModel from './NameSupplementModel';
using com.sap.mdm.bp.CompanyLegalFormModel as CompanyLegalFormModel from './CompanyLegalFormModel';
using com.sap.mdm.bp.NamePrefixModel as NamePrefixModel from './NamePrefixModel';
using com.sap.mdm.bp.NameFormatModel as NameFormatModel from './NameFormatModel';
using com.sap.mdm.bp.GenericModel as GenericModel from './GenericModel';
using com.sap.mdm.bp.BusinessPartnerModel from './BusinessPartner';
using cds.foundation.sap.countrysubdivision.C_Countries from '@sap/reusemodel-countrysubdivision/db/data-model';
using cds.foundation.sap.countrysubdivision.C_Subdivisions from '@sap/reusemodel-countrysubdivision/db/data-model';
using cds.foundation.sap.countrysubdivision.CountryTwoLetterISOCode from '@sap/reusemodel-countrysubdivision/db/types';
using cds.foundation.sap.countrysubdivision.CountrySubdivision from '@sap/reusemodel-countrysubdivision/db/data-model';
using cds.foundation.sap.language.C_Languages from '@sap/reusemodel-language/db/data-model';
using cds.foundation.sap.language.C_LanguageTexts from '@sap/reusemodel-language/db/data-model';
using cds.foundation.sap.language.LanguageTwoLetterISOCode from '@sap/reusemodel-language/db/types';
using cds.foundation.sap.language.LanguageName from '@sap/reusemodel-language/db/types';

context AddressModel {
  @mdm.soap.path : 'BusinessPartner/AddressInformation/Address'
  entity Address {
        @Core.LongDescription           : 'System generated unique identifier for the entity.'
    key Id                         : UUID;
        // Domain ADDR_ADDRESS_TYPE: Organization Address/ Personal Address/ Contact Partner Address/ Address Independent Communication Data / Person
        AddressType                : CommonTypes.AddressType;

        @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/CommunicationPreference/PreferredCommunicationMediumTypeCode'
        PrfrdCommMediumType        : String(3); // table TSAC

        @mdm.child
        to_WorkplaceAddress        : Composition of many Workplace
                                       on to_WorkplaceAddress.Parent_Id = Id;

        @mdm.child
        to_Email                   : Composition of many Email
                                       on to_Email.Parent_Id = Id;

        @mdm.child
        @cds.java.expand                : {using : 'parent-keys'}
        to_MobilePhone             : Association to many Phone
                                       on to_MobilePhone.Parent_Id = Id
                                       and (
                                            to_MobilePhone.NumberType = '2'
                                         or to_MobilePhone.NumberType = '3'
                                       );
        @mdm.child
        @cds.java.expand                : {using : 'parent-keys'}
        to_LandlinePhone           : Association to many Phone
                                       on to_LandlinePhone.Parent_Id = Id
                                       and (
                                            to_LandlinePhone.NumberType = ' '
                                         or to_LandlinePhone.NumberType = '1'
                                       );
        @mdm.child
        to_Phone                   : Composition of many Phone
                                       on to_Phone.Parent_Id = Id;

        @mdm.child
        to_Fax                     : Composition of many Fax
                                       on to_Fax.Parent_Id = Id;

        @mdm.child
        to_Web                     : Composition of many Web
                                       on to_Web.Parent_Id = Id;

        @mdm.child
        to_PostalAddress           : Composition of many PostalAddress
                                       on to_PostalAddress.Parent_Id = Id;

        @mdm.child
        @mdm.parent.association         : 'to_Address'
        to_Person                  : Composition of many Person
                                       on to_Person.Parent_Id = Id;

        @mdm.child
        to_Organization            : Composition of many Organization
                                       on to_Organization.Parent_Id = Id;

        to_DefaultPostalAddress    : Association to one PostalAddress
                                       on  to_DefaultPostalAddress.Parent_Id  = Id
                                       and to_DefaultPostalAddress.ScriptCode = ' ';

        to_DefaultEmail            : Association to one Email
                                       on  to_DefaultEmail.Parent_Id             = Id
                                       and to_DefaultEmail.IsDefaultEmailAddress = true;

        to_DefaultMobilePhone      : Association to one Phone
                                       on  to_DefaultMobilePhone.Parent_Id            = Id
                                       and to_DefaultMobilePhone.NumberType           = '3'
                                       and to_DefaultMobilePhone.IsDefaultPhoneNumber = true;
        //and ( to_DefaultMobilePhone.NumberType	= '2'
        //or to_DefaultMobilePhone.NumberType	= '3' );

        to_DefaultLandlinePhone    : Association to one Phone
                                       on  to_DefaultLandlinePhone.Parent_Id            = Id
                                       and to_DefaultLandlinePhone.NumberType           = '1'
                                       and to_DefaultLandlinePhone.IsDefaultPhoneNumber = true;
        //and ( to_DefaultLandlinePhone.NumberType	= ' '
        //or to_DefaultLandlinePhone.NumberType	= '1' );

        to_DefaultFax              : Association to one Fax
                                       on  to_DefaultFax.Parent_Id          = Id
                                       and to_DefaultFax.IsDefaultFaxNumber = true;

        to_DefaultWeb              : Association to one Web
                                       on  to_DefaultWeb.Parent_Id    = Id
                                       and to_DefaultWeb.IsDefaultUri = true;

        to_DefaultPerson           : Association to one Person
                                       on  to_DefaultPerson.Parent_Id  = Id
                                       and to_DefaultPerson.ScriptCode = ' ';

        to_DefaultOrganization     : Association to one Organization
                                       on  to_DefaultOrganization.Parent_Id  = Id
                                       and to_DefaultOrganization.ScriptCode = ' ';

        to_DefaultWorkplaceAddress : Association to one Workplace
                                       on  to_DefaultWorkplaceAddress.Parent_Id  = Id
                                       and to_DefaultWorkplaceAddress.ScriptCode = ' ';

        to_pcmtc                   : Association to one PreferredCommMediumTypeModel.PreferredCommMediumType
                                       on to_pcmtc.communicationType = PrfrdCommMediumType;

        @mdm.parent
        to_Parent                  : Association to BusinessPartnerModel.AddressInformation
                                       on to_Parent.Address_Id = Id;


  };

  @mdm.soap.path         : 'BusinessPartner/AddressInformation/Address/PostalAddress'
  @mdm.std.soap.cti.flag : ['BusinessPartner/AddressInformation/Address/postalAddressListCompleteTransmissionIndicator']
  entity PostalAddress : managedObject {

    @mdm.personal
    @mdm.semantic.key               : 'AddressRepresentationCode'
    ScriptCode                    : CommonTypes.ScriptCode;

    @Core.LongDescription           : 'Correspondence language (written) for business partners in the Person category'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/CommunicationPreference/CorrespondenceLanguageCode'
    CorrespondenceLanguage        : LanguageTwoLetterISOCode;

    @Core.LongDescription           : 'PO Box number as part of an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxID'
    PoBox                         : String(10);

    @Core.LongDescription           : 'PO Box address without PO Box number flag.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxIDVisibleIndicator'
    PoBoxIsWithoutNumber          : Boolean;

    @Core.LongDescription           : 'Postal code that is required for a unique assignment of the PO box.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxPostalCode'
    PoBoxPostalCode               : CommonTypes.PostalCode;

    @Core.LongDescription           : 'Different PO Box country in address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxDeviatingCountryCode'
    PoBoxDeviatingCountry         : CountryTwoLetterISOCode; //CommonTypes.Country;

    @Core.LongDescription           : 'Different Region for PO Box in an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxDeviatingRegionCode'
    PoBoxDeviatingRegion          : CountrySubdivision;

    @Core.LongDescription           : 'Postal code as part of the address'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/StreetPostalCode'
    PostalCode                    : CommonTypes.PostalCode;

    @Core.LongDescription           : 'Postal code that is assigned directly to one company (= company postal code = major customer postal code).'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/CompanyPostalCode'
    CompanyPostalCode             : CommonTypes.PostalCode;

    @Core.LongDescription           : 'Street name as part of the address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/StreetName'
    StreetName                    : String(60);

    @Core.LongDescription           : 'Additional address field which is printed above the Street line.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/StreetPrefixName'
    StreetPrefixName              : CommonTypes.AddressName;

    @Core.LongDescription           : 'Additional address field which is printed above the Street line.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/AdditionalStreetPrefixName'
    AdditionalStreetPrefixName    : CommonTypes.AddressName;

    @Core.LongDescription           : 'Additional address field which is printed below the Street line.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/StreetSuffixName'
    StreetSuffixName              : CommonTypes.AddressName;

    @Core.LongDescription           : 'Additional address field which is printed under the Street line.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/AdditionalStreetSuffixName'
    AdditionalStreetSuffixName    : CommonTypes.AddressName;

    @Core.LongDescription           : 'House number as part of an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/HouseID'
    HouseNumber                   : CommonTypes.AddressNumber;

    @Core.LongDescription           : 'House number supplement as part of an address'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/AdditionalHouseID'
    HouseNumberSupplementText     : String(10);

    @Core.LongDescription           : 'City code for city/street file'
    @mdm.personal

    CityCode                      : String(12);

    @Core.LongDescription           : 'City name as part of the address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/CityName'
    CityName                      : CommonTypes.AddressName;

    @Core.LongDescription           : 'City or District supplement'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/AdditionalRegionalStructureCityCode'
    AdditionalCityName            : CommonTypes.AddressName;

    @Core.LongDescription           : 'District code for City and Street file'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/DistrictName'
    District                      : CommonTypes.AddressName;

    @Core.LongDescription           : 'The country key contains information which the system uses to check entries such as the length of the postal code or bank account number.'
    @mdm.personal
    @mdm.filterable
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/CountryCode'
    Country                       : CountryTwoLetterISOCode; //CommonTypes.Country;

    @Core.LongDescription           : 'In some countries, the region forms part of the address. The meaning depends on the country.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/RegionCode'
    Region                        : CountrySubdivision;

    @Core.LongDescription           : 'Time zone as part of an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/TimeZoneCode'
    AddressTimeZone               : String(6);

    @Core.LongDescription           : 'Specifies the countys name'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/CountyName'

    County                        : CommonTypes.AddressName;

    @Core.LongDescription           : 'Number or abbreviation of a building in an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/ExtendedBuildingID'
    Building                      : String(20);

    @Core.LongDescription           : 'Floor of the building as more exact specification of an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/FloorID'
    Floor                         : String(10);

    @Core.LongDescription           : 'Apartment or room number in an address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/RoomID'
    RoomNumber                    : String(10);

    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/CareOfName'
    CareOfName                    : CommonTypes.AddressName;

    @Core.LongDescription           : 'The PO box lobby is part of the PO box address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxLobbyName'
    PoBoxLobbyName                : CommonTypes.AddressName;

    @Core.LongDescription           : 'Different city for the PO Box as an address component.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxDeviatingCityName'
    PoBoxDeviatingCityName        : CommonTypes.AddressName;

    @mdm.personal

    Latitude                      : Decimal(15, 12); // SNWDto_LAT

    @mdm.personal

    Longitude                     : Decimal(15, 12); // SNWDto_LONG DEC(15,12)

    @Core.LongDescription           : 'The delivery service is part of the PO box address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxDeliveryServiceID'
    DeliveryServiceNumber         : String(10);

    @Core.LongDescription           : 'The delivery service is part of the PO box address.'
    @mdm.personal
    @mdm.standard.field.soap.target : 'BusinessPartner/AddressInformation/Address/PostalAddress/POBoxDeliveryServiceTypeCode'
    DeliveryServiceTypeCode       : String(4);

    // to_CountryCode					: Association to one CountryModel.Country on to_CountryCode.code = Country;
    to_CountryCode                : Association to one C_Countries
                                      on to_CountryCode.CountryTwoLetterISOCode = Country;
    to_RegionCode                 : Association to one C_Subdivisions
                                      on  to_RegionCode.CountryTwoLetterISOCode = Country
                                      and to_RegionCode.CountrySubdivision      = Region;
    to_PoBoxDeviatingCountryCode  : Association to one C_Countries
                                      on to_PoBoxDeviatingCountryCode.CountryTwoLetterISOCode = PoBoxDeviatingCountry;
    to_PoBoxDeviatingRegionCode   : Association to one C_Subdivisions
                                      on  to_PoBoxDeviatingRegionCode.CountryTwoLetterISOCode = PoBoxDeviatingCountry
                                      and to_PoBoxDeviatingRegionCode.CountrySubdivision      = PoBoxDeviatingRegion;
    to_CorrespondenceLanguageCode : Association to one C_Languages
                                      on to_CorrespondenceLanguageCode.LanguageTwoLetterISOCode = CorrespondenceLanguage;
    @mdm.parent
    to_Parent                     : Association to Address
                                      on to_Parent.Id = Parent_Id;
  };


  entity CommunicationDataText {
        @Core.LongDescription : 'System generated unique identifier for the entity.'
    key Id                    : UUID;
        // @Core.LongDescription: 'Communication Method (Key) (Business Address Services)'
        // CommunicationType : String(3) ;
        Parent_Id             : UUID;
        CommunicationLanguage : LanguageTwoLetterISOCode;
        remark                : String(50)

  };

  @mdm.soap.path         : 'BusinessPartner/AddressInformation/Address/Email'
  @mdm.soap.alt.path     : ['BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Email']
  @mdm.std.soap.cti.flag : [
    'BusinessPartner/AddressInformation/Address/emailListCompleteTransmissionIndicator',
    'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/emailListCompleteTransmissionIndicator'
  ]
  entity Email : managedObject {

    @Core.LongDescription           : 'Flag: this address is the default address'
    @mdm.personal
    IsDefaultEmailAddress : Boolean;

    @Core.LongDescription           : 'Internet mail address, also called e-mail address.'
    @mdm.personal
    @mdm.semantic.key               : 'URI'
    EmailAddress          : CommonTypes.EmailAddress;

    @Core.LongDescription           : 'Validity Start Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Email/ValidityPeriod/StartDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Email/ValidityPeriod/StartDate'
    ]
    ValidityStartDate     : DateTime default timestamp'0001-01-01 00:00:00.0';

    @Core.LongDescription           : 'Validity End Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Email/ValidityPeriod/EndDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Email/ValidityPeriod/EndDate'
    ]
    ValidityEndDate       : DateTime default timestamp'9999-12-31 23:59:59.0';

    @mdm.parent
    to_Parent             : Association to Address
                              on to_Parent.Id = Parent_Id;

    to_EmailNote          : Composition of many CommunicationDataText
                              on to_EmailNote.Parent_Id = Id;

    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Email/UsageDeniedIndicator',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Email/UsageDeniedIndicator'
    ]
    UsageDeniedIndicator  : Boolean;

  };

  @mdm.soap.path         : 'BusinessPartner/AddressInformation/Address/Telephone'
  @mdm.soap.alt.path     : ['BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone']
  @mdm.std.soap.cti.flag : [
    'BusinessPartner/AddressInformation/Address/telephoneListCompleteTransmissionIndicator',
    'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/telephoneListCompleteTransmissionIndicator'
  ]
  entity Phone : managedObject {

    @Core.LongDescription           : 'Standard Sender Address in this Communication Type'
    @mdm.personal
    IsDefaultPhoneNumber       : Boolean; // Default mobile/landline phone is determined by PhoneNumberType

    @Core.LongDescription           : 'The country for the telephone number or fax number is maintained here.'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Telephone/Number/CountryCode',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone/Number/CountryCode'
    ]
    DestinationLocationCountry : CountryTwoLetterISOCode; //CommonTypes.Country;

    @Core.LongDescription           : 'Telephone number, consisting of dialling code and number, but without country dialling code.'
    @mdm.personal
    @mdm.semantic.key               : 'Number/SubscriberID'
    Number                     : CommonTypes.PhoneNumber;

    @Core.LongDescription           : 'This field specifies whether the telephone number is a mobile telephone number.'
    NumberType                 : String(1); // ADto_FLGMOB: fixed values ' ', 1,2,3: phone is: landline, default landline, mobile, default mobile

    @Core.LongDescription           : 'If the telephone number consists of a company number and an extension, the extension should be entered here.'
    @mdm.personal
    @mdm.semantic.key               : 'Number/ExtensionID'
    NumberExtension            : CommonTypes.PhoneNumberExtension;

    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Telephone/UsageDeniedIndicator',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone/UsageDeniedIndicator'
    ]
    UsageDeniedIndicator       : Boolean;
    @mdm.standard.field.soap.target : ['BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone/SMSEnabledIndicator']
    SMSEnabledIndicator        : Boolean;

    @Core.LongDescription           : 'Validity Start Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Telephone/ValidityPeriod/StartDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone/ValidityPeriod/StartDate'
    ]
    ValidityStartDate          : DateTime default timestamp'0001-01-01 00:00:00.0';

    @Core.LongDescription           : 'Validity End Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Telephone/ValidityPeriod/EndDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Telephone/ValidityPeriod/EndDate'
    ]
    ValidityEndDate            : DateTime default timestamp'9999-12-31 23:59:59.0';

    to_CountryCode             : Association to one C_Countries
                                   on to_CountryCode.CountryTwoLetterISOCode = DestinationLocationCountry; //Country;


    @mdm.parent
    to_Parent                  : Association to Address
                                   on to_Parent.Id = Parent_Id;

    to_PhoneNote               : Composition of many CommunicationDataText
                                   on to_PhoneNote.Parent_Id = Id;


  };

  @mdm.soap.path         : 'BusinessPartner/AddressInformation/Address/Facsimile'
  @mdm.soap.alt.path     : ['BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Facsimile']
  @mdm.std.soap.cti.flag : [
    'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/facsimileListCompleteTransmissionIndicator',
    'BusinessPartner/AddressInformation/Address/facsimileListCompleteTransmissionIndicator'
  ]
  entity Fax : managedObject {

    @Core.LongDescription           : 'Standard Sender Address in this Communication Type'
    @mdm.personal
    IsDefaultFaxNumber         : Boolean;

    @Core.LongDescription           : 'The country for the telephone number or fax number is maintained here.'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Facsimile/Number/CountryCode',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Facsimile/Number/CountryCode'
    ]
    DestinationLocationCountry : CountryTwoLetterISOCode; //CommonTypes.Country;

    @Core.LongDescription           : 'Fax number, consisting of dialling code and number, but without country dialling code.'
    @mdm.personal
    @mdm.semantic.key               : 'Number/SubscriberID'
    Number                     : CommonTypes.PhoneNumber;

    @Core.LongDescription           : 'If the fax number consists of a company number and an extension, the extension must be entered here.'
    @mdm.personal
    @mdm.semantic.key               : 'Number/ExtensionID'
    NumberExtension            : CommonTypes.PhoneNumberExtension;

    @Core.LongDescription           : 'Validity Start Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Facsimile/ValidityPeriod/StartDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Facsimile/ValidityPeriod/StartDate'
    ]
    ValidityStartDate          : DateTime default timestamp'0001-01-01 00:00:00.0';

    @Core.LongDescription           : 'Validity End Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Facsimile/ValidityPeriod/EndDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Facsimile/ValidityPeriod/EndDate'
    ]
    ValidityEndDate            : DateTime default timestamp'9999-12-31 23:59:59.0';

    @mdm.parent
    to_Parent                  : Association to Address
                                   on to_Parent.Id = Parent_Id;

    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Facsimile/UsageDeniedIndicator',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Facsimile/UsageDeniedIndicator'
    ]
    UsageDeniedIndicator       : Boolean;


    to_CountryCode             : Association to one C_Countries
                                   on to_CountryCode.CountryTwoLetterISOCode = DestinationLocationCountry; //Country;
    //   to_CountryCode: Association to one CountryModel.Country on to_CountryCode.code = DestinationLocationCountry;
    to_FaxNote                 : Composition of many CommunicationDataText
                                   on to_FaxNote.Parent_Id = Id;
  };

  @mdm.soap.path         : 'BusinessPartner/AddressInformation/Address/Web'
  @mdm.soap.alt.path     : ['BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Web']
  @mdm.std.soap.cti.flag : [
    'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/webListCompleteTransmissionIndicator',
    'BusinessPartner/AddressInformation/Address/webListCompleteTransmissionIndicator'
  ]
  entity Web : managedObject {

    @Core.LongDescription           : 'Flag: this URI is the default URI'
    IsDefaultUri         : Boolean;

    @Core.LongDescription           : 'URI type flag'
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Web/URITypeCode',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Web/URITypeCode'
    ]
    UriType              : String(3);

    @Core.LongDescription           : 'Universal Resource Identifier (URI)'
    @mdm.semantic.key               : 'URI'
    Uri                  : String(2048);


    @Core.LongDescription           : 'Validity Start Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Web/ValidityPeriod/StartDate',
      'BusinessPartnerRelationship/ContactPerson/WorkplaceAddressInformation/Address/Web/ValidityPeriod/StartDate'
    ]
    ValidityStartDate    : DateTime default timestamp'0001-01-01 00:00:00.0';

    @Core.LongDescription           : 'Validity End Date of the master data object'
    @sap.display.format             : 'Date'
    @mdm.personal
    @mdm.standard.field.soap.target : [
      'BusinessPartner/AddressInformation/Address/Web/ValidityPeriod/EndDate',
