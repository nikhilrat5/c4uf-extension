namespace com.sap.mdm.bp;

using cds.foundation.sap.busconfign.BusinessConfiguration as BusConfigurationEntity from '@sap/BusinessConfigurationModel';

context GenericConfigurationsModel {

    @BusConfign.Entity
    entity GenericConfigurations : BusConfigurationEntity {
        key Id                 : UUID;
            ConfigurationName  : String(20) not null;
            ConfigurationValue : String(100);
            AllowedValues      : String(50);
    };

};
