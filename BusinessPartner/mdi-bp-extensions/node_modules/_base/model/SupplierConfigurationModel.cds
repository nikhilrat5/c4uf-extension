  @BusConfign.Entity
  entity AccountDirectory : BusConfigurationEntity {

    key ChartOfAccount                     : String(4);

        to_AccountDirectoryText            : Association to many AccountDirectoryText
                                               on to_AccountDirectoryText.ChartOfAccount = ChartOfAccount;

        to_AccountDirectoryTextDescription : Association to one AccountDirectoryText
                                               on  to_AccountDirectoryTextDescription.ChartOfAccount = ChartOfAccount
                                               and to_AccountDirectoryTextDescription.languageCode   = SESSION_CONTEXT(
                                                 'LOCALE'
                                               );
  }

  @BusConfign.Entity
  entity AccountDirectoryText : BusConfigurationEntity {

    key ChartOfAccount : String(4);
    key languageCode   : LanguageTwoLetterISOCode;
        description    : String(60);
  }

