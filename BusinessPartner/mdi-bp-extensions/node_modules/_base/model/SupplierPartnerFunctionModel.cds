namespace com.sap.mdm.bp;

context SupplierPartnerFunctionModel {

  entity SupplierPartnerDeterminationProcedure {
    key PartnerDeterminationProcedure     : String(4);
        PartnerDeterminationProcedureText : String(20);
  }

  entity SupplierPartnerFunctionProcedure {
    key PartnerDeterminationProcedure : String(4);
    key PartnerFunction               : String(2);
        IsModifiable                  : Boolean;
        IsMandatory                   : Boolean;
  }

  entity SupplierPartnerDeterminationAssignment {
    key SupplierAccountGroupCode      : String(4);
    key PartnerDeterminationProcedure : String(4);
        PartnerSchemaOrgLevel         : String(4);
        PartnerSchemaSubRange         : String(4);
        PartnerSchemaPlantLevel       : String(4);
  }

  entity SupplierPartnerFunction {
    key PartnerFunction     : String(2);
        PartnerFunctionText : String(20);
        PartnerType         : String(2);
  }

  entity SupplierAccountGroupFunctionAssignment {
    key PartnerFunction          : String(2);
    key SupplierAccountGroupCode : String(4);
  }

  entity SupplierPartnerFunctionConversion {
    key PartnerFunction    : String(2);
        PartnerDescription : String(2);
  }
}
