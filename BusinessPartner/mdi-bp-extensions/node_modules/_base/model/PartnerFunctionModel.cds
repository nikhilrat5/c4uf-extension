namespace com.sap.mdm.bp;

context PartnerFunctionModel {

  entity PartnerDeterminationProcedure {
    key PartnerDeterminationProcedure     : String(4);
        PartnerDeterminationProcedureText : String(20);
  }

  entity PartnerFunctionProcedure {
    key PartnerDeterminationProcedure : String(4);
    key PartnerFunction               : String(2);
        IsModifiable                  : Boolean;
        IsMandatory                   : Boolean;
  }

  entity PartnerDeterminationProcedureAssignment {
    key CustomerAccountGroupCode      : String(4);
    key PartnerDeterminationProcedure : String(4);
  }

  entity PartnerFunction {
    key PartnerFunction     : String(2);
        PartnerFunctionText : String(20);
        PartnerType         : String(2);
  }

  entity CustomerAccountGroupFunctionAssignment {
    key PartnerFunction          : String(2);
    key CustomerAccountGroupCode : String(4);
  }

  entity PartnerFunctionConversion {
    key PartnerFunction    : String(2);
        PartnerDescription : String(2);
  }
}
