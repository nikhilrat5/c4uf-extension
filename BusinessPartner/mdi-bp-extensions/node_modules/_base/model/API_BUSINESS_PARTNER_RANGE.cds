using com.sap.mdm.bp.NumberGeneratorModel as NumberGeneratorModel from './NumberGeneratorModel';

service API_BUSINESS_PARTNER_RANGE {

  @Capabilities : {
    Insertable : false,
    Updatable  : true,
    Deletable  : false
  }
  view NumberGenerator as
    select from NumberGeneratorModel.NumberGenerator {
      key NumberGenerator.numberRangeName as numberRangeName,
          NumberGenerator.startValue      as startValue,
          NumberGenerator.currentValue    as currentValue,
          NumberGenerator.endValue        as endValue
    }
    where
      numberRangeName = 'BusinessPartnerRange';

};
