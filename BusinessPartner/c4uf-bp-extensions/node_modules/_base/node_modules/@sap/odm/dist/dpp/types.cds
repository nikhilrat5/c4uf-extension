namespace sap.odm.dpp;

/**
 * Display Id for DPP entities.
 */
type DisplayId: String(30);

/**
 * Version of an entity with major and minor version
 * information.
 */
type Version: {
  /**
   * Major part of version.
   */
  major: Integer;
  /**
   * Minor part of version.
   */
  minor: Integer;
};
