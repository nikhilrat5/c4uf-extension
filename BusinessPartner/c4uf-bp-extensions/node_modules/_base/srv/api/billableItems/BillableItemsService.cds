
service BillableItemsService @(
    path: '/api/billing/v1/billableItems',
    protocol : 'odata',
    requires : 'authenticated-user',
    version: 'v1'
) {
     @(restrict : [
        {
            grant : ['WRITE'],
            to    : ['API.Write','Admin','jobcallback']
        },
        {
            grant : ['READ'],
            to    : ['API.Read','Admin','jobcallback']
        }
    ])
    action transfer();
};